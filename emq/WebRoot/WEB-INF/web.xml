<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN" "http://java.sun.com/dtd/web-app_2_3.dtd">
<web-app id="WebApp_1258537986765">
	<display-name>EMQ</display-name>
	<context-param>
		<param-name>contextConfigLocation</param-name>
		<param-value>
			/WEB-INF/classes/applicationContext.xml
		</param-value>
	</context-param>
	<context-param>
		<param-name>webAppRootKey</param-name>
		<param-value>/EMQ.war</param-value>
	</context-param>
	<context-param>
		<param-name>log4jConfigLocation</param-name>
		<param-value>/WEB-INF/classes/log4j.xml</param-value>
	</context-param>
	<filter>
		<filter-name>Set Character Encoding</filter-name>
		<filter-class>
			com.icss.km.common.servlet.SetCharacterEncodingFilter
		</filter-class>
		<init-param>
			<param-name>encoding</param-name>
			<param-value>GBK</param-value>
		</init-param>
		<init-param>
			<param-name>ignore</param-name>
			<param-value>true</param-value>
		</init-param>
	</filter>
	<!-- request 映射路径  -->
	<filter>
		<filter-name>RequestMappingFilter</filter-name>
		<filter-class>
			com.emq.ui.filter.RequestMappingFilter
		</filter-class>
	</filter>
	<filter-mapping>
		<filter-name>RequestMappingFilter</filter-name>
		<url-pattern>/gis/main-map/*</url-pattern>
	</filter-mapping>
	<!-- 调用缩略图 -->
	<filter-mapping>
		<filter-name>RequestMappingFilter</filter-name>
		<url-pattern>/gis/preview-map/*</url-pattern>
	</filter-mapping>
	<filter-mapping>
		<filter-name>Set Character Encoding</filter-name>
		<servlet-name>action</servlet-name>
	</filter-mapping>
	<servlet>
		<servlet-name>mapxtreme</servlet-name>
		<servlet-class>
			com.mapinfo.mapxtreme.MapXtremeServlet
		</servlet-class>
		<load-on-startup>4</load-on-startup>
	</servlet>
	<servlet>
		<servlet-name>mxtj_image</servlet-name>
		<servlet-class>
			com.mapinfo.mapxtreme.ImageHandlerServlet
		</servlet-class>
		<load-on-startup>5</load-on-startup>
	</servlet>
	<servlet>
		<servlet-name>mxtj_metadata</servlet-name>
		<servlet-class>
			com.mapinfo.mapxtreme.MetadataHandlerServlet
		</servlet-class>
		<load-on-startup>6</load-on-startup>
	</servlet>
	<servlet>
		<servlet-name>mxtj_vector</servlet-name>
		<servlet-class>
			com.mapinfo.mapxtreme.VectorHandlerServlet
		</servlet-class>
		<load-on-startup>7</load-on-startup>
	</servlet>
	<servlet>
		<servlet-name>log4jConfigServlet</servlet-name>
		<servlet-class>
			org.springframework.web.util.Log4jConfigServlet
		</servlet-class>
		<load-on-startup>1</load-on-startup>
	</servlet>
	<servlet>
		<servlet-name>SpringContextServlet</servlet-name>
		<servlet-class>
			org.springframework.web.context.ContextLoaderServlet
		</servlet-class>
		<load-on-startup>2</load-on-startup>
	</servlet>
	<!-- 系统初始化servlet -->
	<servlet>
		<servlet-name>EMQContextServlet</servlet-name>
		<servlet-class>
			com.emq.ui.servlet.InitServlet
		</servlet-class>
		<load-on-startup>3</load-on-startup>
	</servlet>
	<servlet>
		<servlet-name>action</servlet-name>
		<servlet-class>
			org.apache.struts.action.ActionServlet
		</servlet-class>
		<init-param>
			<param-name>config</param-name>
			<param-value>/WEB-INF/struts-config.xml</param-value>
		</init-param>
		<init-param>
			<param-name>debug</param-name>
			<param-value>true</param-value>
		</init-param>
		<init-param>
			<param-name>detail</param-name>
			<param-value>true</param-value>
		</init-param>
		<load-on-startup>0</load-on-startup>
	</servlet>
	<!-- dwr -->
	<servlet>
		<servlet-name>dwr-invoker</servlet-name>
		<servlet-class>
			org.directwebremoting.servlet.DwrServlet
		</servlet-class>
		<init-param>
			<param-name>debug</param-name>
			<param-value>true</param-value>
		</init-param>
	</servlet>
	<servlet>
		<servlet-name>MainMapServlet</servlet-name>
		<display-name>主地图Servlet</display-name>
		<description>主地图控制接口</description>
		<servlet-class>
			com.emq.ui.servlet.MainMapServlet
		</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>PreviewMapServlet</servlet-name>
		<display-name>缩略地图Servlet</display-name>
		<description>缩略地图控制接口</description>
		<servlet-class>
			com.emq.ui.servlet.PreviewMapServlet
		</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>action</servlet-name>
		<url-pattern>*.do</url-pattern>
	</servlet-mapping>
	<!-- mapxtreme -->
	<servlet-mapping>
		<servlet-name>mapxtreme</servlet-name>
		<url-pattern>/servlet/mapxtreme</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>mapxtreme</servlet-name>
		<url-pattern>/mapxtreme</url-pattern>
	</servlet-mapping>
	<!-- dwr -->
	<servlet-mapping>
		<servlet-name>dwr-invoker</servlet-name>
		<url-pattern>/dwr/*</url-pattern>
	</servlet-mapping>
	<!-- 地图控制 接口 -->
	<servlet-mapping>
		<servlet-name>MainMapServlet</servlet-name>
		<url-pattern>/servlet/MainMapServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>PreviewMapServlet</servlet-name>
		<url-pattern>/servlet/PreviewMapServlet</url-pattern>
	</servlet-mapping>
	<!-- mapinfo标签库 -->
	<taglib>
		<taglib-uri>/mapinfo</taglib-uri>
		<taglib-location>/WEB-INF/tlds/mapinfo_4_0.tld</taglib-location>
	</taglib>
	<taglib>
		<taglib-uri>/mapxtreme</taglib-uri>
		<taglib-location>
			/WEB-INF/tlds/mapxtreme_4_0.tld
		</taglib-location>
	</taglib>
	<taglib>
		<taglib-uri>/wms</taglib-uri>
		<taglib-location>/WEB-INF/tlds/wms_4_7.tld</taglib-location>
	</taglib>
	<taglib>
		<taglib-uri>/mapmarker</taglib-uri>
		<taglib-location>
			/WEB-INF/tlds/mapmarker_4_0.tld
		</taglib-location>
	</taglib>
	<taglib>
		<taglib-uri>/routing</taglib-uri>
		<taglib-location>/WEB-INF/tlds/routing_4_0.tld</taglib-location>
	</taglib>
	<!-- struts 标签库 -->
	<taglib>
		<taglib-uri>/WEB-INF/struts-bean.tld</taglib-uri>
		<taglib-location>/WEB-INF/tlds/struts-bean.tld</taglib-location>
	</taglib>
	<taglib>
		<taglib-uri>/WEB-INF/struts-html.tld</taglib-uri>
		<taglib-location>/WEB-INF/tlds/struts-html.tld</taglib-location>
	</taglib>
	<taglib>
		<taglib-uri>/WEB-INF/struts-logic.tld</taglib-uri>
		<taglib-location>/WEB-INF/tlds/struts-logic.tld</taglib-location>
	</taglib>
	<taglib>
		<taglib-uri>/WEB-INF/struts-tiles.tld</taglib-uri>
		<taglib-location>/WEB-INF/tlds/struts-tiles.tld</taglib-location>
	</taglib>
	<taglib>
		<taglib-uri>/WEB-INF/struts-nested.tld</taglib-uri>
		<taglib-location>/WEB-INF/tlds/struts-nested.tld</taglib-location>
	</taglib>
</web-app>
